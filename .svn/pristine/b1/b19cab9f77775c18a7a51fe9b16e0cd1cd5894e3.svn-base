#import "BaseModel.h"

@interface MerchantOrderDetailModel : BaseModel
@property(nonatomic,copy)NSString *orderId;////订单id
@property(nonatomic,copy)NSString *number;//编号
@property(nonatomic,copy)NSString *statusName;// 订单状态
@property(nonatomic,copy)NSString *orderTime;// 下单时间
@property(nonatomic,copy)NSString *memberID;// 用户id
@property(nonatomic,copy)NSString *name;// 收货人姓名
@property(nonatomic,copy)NSString *sex;// 性别
@property(nonatomic,copy)NSString *memberCode;// 联系电话
@property(nonatomic,copy)NSString *address;// 收货地址
@property(nonatomic,copy)NSString *xqLocation; // 收货人小区坐标
@property(nonatomic,copy)NSString *didLocation;// 商家店铺坐标
@property(nonatomic,copy)NSString *sendFee;// 配送费
@property(nonatomic,copy)NSString *youhuiMoney;// 优惠金额
@property(nonatomic,copy)NSString *payAmount;// 实付金额
@property(nonatomic,copy)NSString *orderAmount;// 订单总金额（优惠前）
@property(nonatomic,copy)NSArray *goodsInfo;//买的东西

@property(nonatomic,copy)NSString *have_kuaiDiYuan;// 判断是否有快递员，true有，false没有
@property(nonatomic,copy)NSString *senderName;// 现在没有快递员姓名
@property(nonatomic,copy)NSString *typeName;// 用户类型说明
@property(nonatomic,copy)NSString *telphone;// 快递员电话
@end
@interface MerchantOrderDetailGoodsInfoModel : BaseModel
@property(nonatomic,copy)NSString *goodsName;// 商品名称
@property(nonatomic,copy)NSString *price;// 单价
@property(nonatomic,copy)NSString *totalPrice;// 总价
@property(nonatomic,copy)NSString *count;// 数量
@end
